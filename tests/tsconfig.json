{
  "compilerOptions": {
    "noEmit": true,
    "target": "es5",
    "rootDir": "..",
    "baseUrl": "./",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "esnext",
      "dom"
    ],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "paths": {
      "*": [
        "../*",
        "*"
      ]
    }
  }
}
